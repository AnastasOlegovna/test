def count():
    """
    :return: произведение цифр числа, которое ввел пользователь, за исключением 0
    """
    y = input('Введите целое положительное число:')  # 123
    f = 1
    for i in y:
        if min(y) != 0:
            f = f * int(i)
    print(f'Произведение цифр числа {y}:', f)


count()

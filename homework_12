def count(y):
    """
    :return: произведение цифр числа, которое ввел пользователь, за исключением 0
    """
    f = 1
    while y > 0:
        s = y % 10  # Получение остатка от деления числа y на 10
        if s != 0:
            f *= s
        y = y // 10     # делим без остатка на 10
    return f

count(11023)
